{
  "name": "GitHub Mention Highlighter",
  "version": "0.1.0",
  "description": "Highlight user and team mentions on GitHub",
  "permissions": ["https://github.com/*", "https://*.githubapp.com/*"],
  "content_scripts": [{
    "matches": ["https://github.com/*", "https://*.githubapp.com/*"],
    "js": ["vendor/jquery/dist/jquery.js","dist/script.js"],
    "css": ["dist/style.css"]
  }],
  "manifest_version": 2
}
