{
  "name": "GitHub Mention Highlighter",
  "version": "0.0.2",
  "description": "Highlight user and team mentions on GitHub",
  "permissions": ["https://github.com/*"],
  "content_scripts": [{
    "matches": ["https://github.com/*"],
    "js": ["vendor/jquery/dist/jquery.js","script.js"],
    "css": ["style.css"]
  }],
  "manifest_version": 2
}
